{
    "name": "bangsystems/dotenv-editor",
    "description": "The .env file editor tool for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "dotenv", "dotenv-editor", "bangsystems"],
    "authors": [
        {
            "name": "Luke Hebblethwaite",
            "email": "luke@nexuscare.co.uk"
        },
        {
            "name": "Jackie Do",
            "email": "anhvudo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0|^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "illuminate/contracts": "^12.0|^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "illuminate/support": "^12.0|^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "jackiedo/path-helper": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bangsystems\\DotenvEditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Bangsystems\\DotenvEditor\\DotenvEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "Bangsystems\\DotenvEditor\\Facades\\DotenvEditor"
            }
        }
    },
    "minimum-stability": "stable"
}
